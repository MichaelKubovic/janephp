{
    "name": "jane-php/open-api-runtime",
    "description": "Jane OpenAPI Runtime Library, dependencies and utility class for a library generated by jane/openapi",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        },
        {
            "name": "Baptiste Leduc",
            "email": "baptiste.leduc@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Jane\\OpenApiRuntime\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.2",
        "jane-php/json-schema-runtime": "^5.0",
        "php-http/client-common": "^1.4 || ^2.0",
        "php-http/multipart-stream-builder": "^1.0",
        "symfony/options-resolver": "^3.1 || ^4.0",
        "php-http/httplug": "^1.0 || ^2.0",
        "php-http/message-factory": "^1.0.2",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit --coverage-text --coverage-clover=build/coverage.xml"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
